{
  "title": "JJ: Change F6 to shift-option-command",
  "rules": [
    {
      "description": "Change F6 to shift-option-command",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f6",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_option",
              "modifiers": [
                "left_shift",
                "right_command"
              ]
            }
          ]
        }
      ]
    }
  ]
}
